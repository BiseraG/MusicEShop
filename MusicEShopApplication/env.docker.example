# Copy to env.docker and fill in your values.
# Run with: docker compose up

# Required - SQL Server connection string (double underscore for nested config)
CONNECTIONSTRINGS__DEFAULTCONNECTION=Server=YOUR_SERVER;Database=YOUR_DB;User Id=YOUR_USER;Password=YOUR_PASSWORD;

# Optional - Stripe (overrides appsettings.json defaults)
Stripe__SecretKey=sk_test_your_key
Stripe__PublishableKey=pk_test_your_key
